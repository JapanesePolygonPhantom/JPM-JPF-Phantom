file=JPF_external.inp
$OMP=0
[ Title ]
# ----------------------------------------------------------------------------
# Example input file for calculations of organ doses due to external exposure
# using the Japanese Polygon mesh-type Female (JPF) phantom of adult Japanese 
# on the Particle and Heavy Ion Transport code System (PHITS). 
#
# Compiled by K. Sato on 2024/07/1. 
# ----------------------------------------------------------------------------
[ Parameters ]
 icntl    =           0     # (D=0) 3:ECH 5:NOR 6:SRC 7,8:GSH 11:DSH 12:DUMP
 maxcas   =     1000000     # (D=10) number of particles per one batch
 maxbch   =          10     # (D=10) number of batches
 itetvol  =           0     # (D=0) =1 Volume calculation for tetrahedrons
 ntetelem =       20000     # (D=200) Number of tetra element allowed in sub-block
 ntetsurf =        5000     # (D=100) Number of outer surfaces of tetrahedron geometry 
                            #  allowed for each sub-space of the container box.
 maxbnk   =     1000000     # (D=10000) maximum bank memory length
 itetra   =           0     # (D=0) tetra data is read(=1)/write(=2) on binary
$ file(1) = (path to PHITS install folder)
 file(6)  = JPF_external.out             # (D=phits.out) general output file name
$ file(7)  = (path to 'xsdir.jnd' file)   # nuclear data input file name
$ file(14) = (path to 'trxcrd.dat' file)  # photon decay-data library file name
$ file(20) = (path to egs folder)         # Directory library data for EGS5
 negs     =           1     # (D=-1) 1:EGS
 ipegs    =           0     # (D=0) Output PEGS5.inp: Yes, Execute PEGS5: Yes, Execute PHITS: Yes

[ Source ]
$
$ ----------------------------------------------------------------------------
$ Radiation and its energy 
$ ----------------------------------------------------------------------------
$
     proj =  photon         # kind of incident particle         
       e0 = 1.0             # energy of beam [MeV]
$
$ ----------------------------------------------------------------------------
$ External source geometry for Anterior-Posterior (AP) irradiation to the JPF phantom. 
$ ----------------------------------------------------------------------------
$
   s-type =   2             # mono-energetic rectangular-solid source
       x0 =  -35.000        # minimum position of x-axis [cm]
       x1 =   35.000        # maximum position of x-axis [cm]
       y0 =  -20.000        # minimum position of y-axis [cm]
       y1 =  -20.000        # maximum position of y-axis [cm]
       z0 =  -85.000        # minimum position of z-axis [cm]
       z1 =   85.000        # maximum position of z-axis [cm]
      dir =    0.0000       # z-direction of beam [cosine]
      phi =   90.000        # azimuthal angle of beam [degree]
      dom =    0.0000       # spread of beam direction [degree]
$
$ ----------------------------------------------------------------------------
$ End source 
$ ----------------------------------------------------------------------------

[ Material ]
$ ----------------------------------------------------------------------------
$ Elemental composition and mass fraction data for organs and tissues of
$ the JPF phantom.
$ ----------------------------------------------------------------------------
 infl: {../Phantom/JPF/JPF.material}
$

[ Surface ]
 10  rpp  -105 105 -105 105 -105 105  $ Phantom box small
 20  rpp  -110 110 -110 110 -110 110  $ Phantom box big
 90  box -1000 -1000 -1000 2000 0 0 0 2000 0 0 0 2000  $ World boundary

[ Cell ]
$ ----------------------------------------------------------------------------
$  Cell data for the JPF phantom
$ ----------------------------------------------------------------------------
 infl: {../Phantom/JPF/JPF.cell}

